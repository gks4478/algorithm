    "from sys import stdin\n",
    "input= stdin.readline\n",
    "\n",
    "n= int(input())\n",
    "num= 3\n",
    "\n",
    "for _ in range(n-1):\n",
    "    num += num-1\n",
    "\n",
    "print(num*num)"
